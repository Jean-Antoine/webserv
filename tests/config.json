{
	"server": [
		{
			"host": "127.0.0.1",
			"port": 9999,
			"server_names": ["example.com", "www.example.com"],
			"client_max_body_size": 4194304,
			"default_error_pages": {
				"404": "/tmp/www/custom_errors/404.html"
			},
			"session_req_cnt": {
				"enabled": false,
				"timeout": 60
			},
			"routes": [
				{
					"path": "/",
					"methods": ["GET"],
					"root": "/tmp/www/",
					"directory_listing": true
				},
				{
					"path": "/webserv_test",
					"methods": ["GET", "POST"],
					"root": "/tmp/www/webserv_test",
					"default_file": "index.html",
					"directory_listing": true,
					"redirection": {
						"enabled": false,
						"url": "127.0.0.1"
					},
					"cgi": {
						"php": "/usr/bin/php-cgi",
						"py": "/usr/bin/python"
					},
					"uploads": {
						"enabled": false,
						"directory": "/tmp/www/uploads"
					}
				},
				{
					"path": "/images",
					"methods": ["GET", "DELETE"],
					"root": "/tmp/www/images",
					"directory_listing": true
				},
				{
					"path": "/uploads",
					"methods": ["GET", "POST", "DELETE"],
					"root": "/tmp/www/uploads",
					"directory_listing": true,
					"uploads": {
						"enabled": true,
						"directory": "/tmp/www/images"
					}
				},
				{
					"path": "/styles",
					"methods": ["GET"],
					"root": "/tmp/www/styles",
					"directory_listing": false
				}
			]
		},
		{
			"host": "127.0.0.1",
			"port": 9999,
			"server_names": ["www.example-2.com"],
			"client_max_body_size": 1024,
			"routes": [
				{
					"path": "/",
					"methods": ["GET", "POST"],
					"root": "/tmp/www2",
					"default_file": "index.html",
					"directory_listing": false
				}
			]
		}
	]
}
